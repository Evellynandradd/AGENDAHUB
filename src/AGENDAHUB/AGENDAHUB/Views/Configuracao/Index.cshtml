@model IEnumerable<AGENDAHUB.Models.Configuracao>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Edit">Dados Cadastrais</a>
</p>

<p>
    <a asp-controller="Profissionais" asp-action="Index">Cadastrar Usuario</a>

</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UsuarioID)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
      


                @if (Model != null)
                {
                    foreach (var item in Model)
                    {
                        if (item != null)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.UsuarioID)
                                </td>
                                <td>
                                    <a asp-action="Edit" asp-route-id="@item.ID_Configuracao">Edit</a> |
                                    <a asp-action="Details" asp-route-id="@item.ID_Configuracao">Details</a> |
                                    <a asp-action="Delete" asp-route-id="@item.ID_Configuracao">Delete</a>
                                </td>
                            </tr>
                        }
                    }
                }
                
        
    </tbody>
</table>
